<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="172.0" prefWidth="172.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.courseworkwidgets.WeatherController">
    <children>
        <Circle fill="DODGERBLUE" layoutX="86.0" layoutY="86.0" radius="60.0" stroke="BLACK" strokeType="INSIDE"
                visible="false"/>
        <ImageView fx:id="weatherImage" fitHeight="80.0" fitWidth="80.0" layoutX="46.0" layoutY="16.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <Label fx:id="cityNameLable" alignment="CENTER" layoutX="26.0" layoutY="84.0" prefHeight="60.0"
               prefWidth="120.0" styleClass="text" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
